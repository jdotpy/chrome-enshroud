{
    "manifest_version": 2,
    "version": "1.0.1",
    "name": "Enshroud",
    "description": "Darken up things",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
        "default_title": "Terminalize",
        "default_icon": "images/black-box-32.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },  
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["dark-styles.css"],
            "js": ["content-script.js"]
        }
    ]
}
